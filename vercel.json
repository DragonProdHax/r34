{
  "functions": {
    "api/wisp.js": {
      "runtime": "nodejs20.x"
    },
    "api/bare.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/uv/(.*)",
      "destination": "/public/uv/$1"
    },
    {
      "source": "/baremux/(.*)",
      "destination": "/public/baremux/$1"
    },
    {
      "source": "/libcurl/(.*)",
      "destination": "/public/libcurl/$1"
    },
    {
      "source": "/scram/(.*)",
      "destination": "/public/scram/$1"
    },
    {
      "source": "/wisp/(.*)",
      "destination": "/api/wisp"
    },
    {
      "source": "/seal/(.*)",
      "destination": "/api/bare"
    },
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/public/assets/$1"
    }
  ]
}
