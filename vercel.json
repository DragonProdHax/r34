{
  "rewrites": [
    {
      "source": "/uv/(.*)",
      "destination": "/public/uv/$1"
    },
    {
      "source": "/baremux/(.*)",
      "destination": "/public/baremux/$1"
    },
    {
      "source": "/libcurl/(.*)",
      "destination": "/public/libcurl/$1"
    },
    {
      "source": "/scram/service/(.*)",
      "destination": "/api/bare"
    },
    {
      "source": "/scram/(.*)",
      "destination": "/public/scram/$1"
    },
    {
      "source": "/seal/(.*)",
      "destination": "/api/bare"
    },
    {
      "source": "/src/(.*)",
      "destination": "/src/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/public/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
